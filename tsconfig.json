{
  "compilerOptions": {
    "module": "commonjs",
    "rootDir": "src",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "outDir": "out",
    "sourceMap": true,
    "esModuleInterop": true
  },
  "include": ["src/"],
  "exclude": ["node_modules"]
}
